RewriteEngine On

##RewriteCond %{REQUEST_FILENAME} !=f
##RewriteCond %{REQUEST_FILENAME} !=d
##RewriteRule ^(.*)$ index.php?url=$1 [NC,L]


RewriteRule ^salut$ index.php

RewriteRule ^article/([a-z0-9-]+)-([0-9]+)$ article.php?name=$1&id=$2 [QSA]


RewriteRule ^api/posts/([0-9]+)/([a-z0-9-]+)(/?)([0-9]*)$ tests/article.php?p=$2&id=$4&mainId=$1
RewriteRule ^api/([a-z0-9-]+)(/?)([0-9]*)$ tests/article.php?p=$1&id=$3